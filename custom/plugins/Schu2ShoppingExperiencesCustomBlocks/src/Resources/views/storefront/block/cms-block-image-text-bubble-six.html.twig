{% block block_image_text_bubble_six %}
    <style>
        .cms-block-image-text-bubble-six .cms-element-image .cms-image-container, .cms-block-image-bubble-row-six .cms-element-image .cms-image-container {
            height: 150px;
            width: 150px;
            margin: auto;
            border-radius: 50%;
            -webkit-background-clip: padding-box;
            background-clip: padding-box;
            overflow: hidden;
        }
        .cms-block-image-text-bubble-six .cms-element-text, .cms-block-image-text-row-six .cms-element-text {
            padding-top: 20px;
        }
        @media screen and (max-width: 991px) {
            .cms-block-image-text-bubble-six .cms-image-container {
                min-height: 300px !important;
            }
            .cms-block-image-text-bubble-six .cms-element-image .cms-image-container, .cms-block-image-bubble-row-six .cms-element-image .cms-image-container {
                height: 200px;
                width: 300px;
            }

        }
    </style>
    {% set columns = 6 %}
    {# @deprecated tag:v6.5.0 - Use "element.id" directly instead. #}
    {% set id = element.id %}

    {% block block_image_text_bubble_six_left %}
        <div class="col-lg-2 col-sm-6">
            {% block block_image_text_bubble_six_left_image %}
                {% set element = block.slots.getSlot('left-image') %}

                <div data-cms-element-id="{{ element.id }}">
                    {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
                </div>
