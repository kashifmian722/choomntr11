<script>
    function sendWhatsAppMessage() {
        // Get the current page URL
        var currentPageURL = window.location.href;

        // Get the user-entered message from the input field
        var userMessage = document.getElementById('whatsappMessageInput').value;

        // Construct the WhatsApp message with the user message and current page URL
        var whatsappMessage = userMessage + " " + currentPageURL;

        // Construct the WhatsApp URL with the message and phone number
        var whatsappURL = "https://wa.me/{{ page.merchant.customFields.whatsapp }}?text=" + encodeURIComponent(whatsappMessage);

        // Open the WhatsApp URL in a new tab/window
        window.open(whatsappURL, '_blank');

        // Prevent the form from submitting
        return false;
    }

